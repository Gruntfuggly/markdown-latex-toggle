{
    "name": "markdown-latex-toggle",
    "displayName": "Markdown/LaTeX Toggle",
    "description": "Toggle between Markdown and LaTeX formatting",
    "icon": "icon.png",
    "version": "0.0.1",
    "publisher": "Gruntfuggly",
    "repository": "https://github.com/Gruntfuggly/markdown-latex-toggle",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "markdown-latex-toggle.markdown-to-latex",
                "title": "Convert Markdown to LaTeX"
            },
            {
                "command": "markdown-latex-toggle.latex-to-markdown",
                "title": "Convert LaTeX to Markdown"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Markdown/LaTeX Toggle! configuration",
            "properties": {}
        }
    },
    "devDependencies": {
        "vscode": "^1.0.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
